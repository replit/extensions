{
  "name": "@replit/extensions",
  "version": "0.28.0",
  "description": "",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "default": "./dist/index.js"
      },
      "default": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.js"
      },
      "default": "./dist/react/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "unpkg": "dist/index.global.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsup src/index.ts src/react/index.ts  --sourcemap --dts --format esm,cjs,iife --global-name replit",
    "lint": "npx prettier --write src/* && cd example && npx prettier --write .",
    "lint:check": "npx prettier -l src/* && cd example && npx prettier -l .",
    "type:check": "tsc --noEmit",
    "dev": "npm run build && cd ./example && npx vite",
    "prepublishOnly": "npm run build",
    "generate:docs": "npx typedoc src/index.ts --json docs/main.json --plugin ./util/signature-plugin/index.cjs"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^18.0.28",
    "comlink": "^4.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.0.1",
    "esbuild": "^0.15.18",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "tsup": "^6.6.3",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
