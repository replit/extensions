{
  "name": "@replit/extensions",
  "version": "0.32.1",
  "description": "",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/extensions/index.js",
      "require": "./dist/extensions/index.cjs",
      "types": "./dist/extensions"
    },
    "./react": {
      "import": "./dist/extensions-react/index.js",
      "require": "./dist/extensions-react/index.cjs",
      "types": "./dist/extensions-react"
    }
  },
  "type": "module",
  "types": "dist/extensions",
  "unpkg": "dist/extensions/index.global.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && npx tsup",
    "lint": "npx prettier --write src/* && cd example && npx prettier --write .",
    "lint:check": "npx prettier -l src/* && cd example && npx prettier -l .",
    "type:check": "tsc --noEmit",
    "dev": "npm run build && cd ./example && npx vite",
    "prepublishOnly": "npm run build",
    "generate:docs": "npx typedoc src/index.ts --json docs/main.json --plugin ./util/signature-plugin/index.cjs"
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/extensions-react/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "keywords": ["replit", "extensions", "api-client", "react"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@codemirror/state": "^6.2.0",
    "@types/react": "^18.0.28",
    "comlink": "^4.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.0.1",
    "esbuild": "^0.15.18",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "tsup": "^6.6.3",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
