{
  "name": "@replit/extensions",
  "version": "0.18.0",
  "description": "",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "default": "./dist/index.js"
      },
      "default": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.js"
      },
      "default": "./dist/react/index.cjs"
    }
  },
  "type": "module",
  "unpkg": "dist/index.global.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup src/index.ts src/react/index.ts  --sourcemap --dts --format esm,cjs,iife",
    "lint": "npx prettier --write src/*",
    "dev": "npm run build && npx vite ./example",
    "prepublishOnly": "npm run build",
    "generate:docs": "npx typedoc src/index.ts --json docs/main.json --plugin ./util/signature-plugin"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "comlink": "^4.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.0.1",
    "esbuild": "^0.15.18",
    "prettier": "^2.7.1",
    "tsup": "^6.6.3",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
